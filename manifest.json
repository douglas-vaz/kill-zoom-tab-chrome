{
  "manifest_version": 2,
  "name": "Zoom Tab Cleaner",
  "description": "This extension will auto close Zoom tabs",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "alarms",
    "https://*.zoom.us/"
  ]
}
